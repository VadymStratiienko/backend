{
  "version": 2,
  "env": {
    "PORT"="5000",
    "BD_PORT":"5432",
    "DB_HOST":"localhost",
    "DB_USER":"nest-pr",
    "DB_PASSWORD":"nest",
    "DB_NAME":"nest-pr",
    "SECRET":"VeryHardSecret",
    "EXPIRE_JWT":"24h"
  },
  "builds": [
    {
      "src": "src/main.ts",
